
package system_rfid;

/**
 *
 * @author usuario
 */


import javax.swing.JFrame;



public class Ventana extends JFrame {
    
 
        public Ventana(String titulo){
            
            setVisible(true);
            setTitle(titulo);
            setLocation(100, 10);
            setSize(700, 600);
            setResizable(false);
            setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
          

        }

       
    
}
