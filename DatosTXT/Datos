         System.out.println("O2 Lectores Inutiles "     +li);
         System.out.println("O3 Tags Cubiertos "        +tc+"%");
         System.out.println("O4 Lectores Fuera de Area "+lfa);
         System.out.println("O5 Lectores Redundantes "  +lr);
         System.out.println("O6 Tags Sobrelapados "     +ts);
         System.out.println("  ");

}


